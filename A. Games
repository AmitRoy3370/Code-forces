import java.util.Scanner;

public class Abhishek {
    public static void main(String[] args) throws Exception {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        int[] freq = new int[101];
        for (int i = 0; i < n; i++) {
            arr[i] = sc.nextInt();
            freq[sc.nextInt()]++;
        }
        int count = 0;
        for (int i = 0; i < n ; i++) {
            count+=freq[arr[i]];
        }
        System.out.println(count);


    }

}
