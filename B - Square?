
import java.util.Scanner;public class Main {public static void main(String[] args) {Scanner scanner=new Scanner(System.in);int t= scanner.nextInt();while (t--!=0){int a= scanner.nextInt(), b= scanner.nextInt(),c= scanner.nextInt(),d= scanner.nextInt();if ((a==c && b+d==a)||(a==d && c+b==d)||(b==c && a+d==b)||(b==d && a+c==d))System.out.println("Yes");else System.out.println("No");}}}
