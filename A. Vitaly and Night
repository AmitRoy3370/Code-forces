import java.util.*;

public class Hackerrank{
	
	public static void main(String[] args) {
		
		Scanner sc1 = new Scanner(System.in);
		
		int n = sc1.nextInt();
		int m = sc1.nextInt();
		
		int t = 0;
		
		for(int i=0; i<n; i++) {
			
			for(int j=0; j<m; j++) {
				
				int a = sc1.nextInt();
				int b = sc1.nextInt();
								
				if(a+b>0) {
					t++;
				}
				
			}
			
		}
		
		System.out.println(t);
		
	}
	
}
