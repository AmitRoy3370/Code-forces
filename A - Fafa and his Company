
import java.util.Scanner;

public class FafaCompany {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        int c=0;
        for (int i = 2; i <=n; i++) {
           if((n-i)%i==0 && (n-i)>=1){
               ++c;
           }
        }
        System.out.println(c+1);
    }
}
