import java.util.Scanner;

public class Main{
    
    static Scanner in=new Scanner(System.in);
    
    public static void main(String[] amit){
        
   int n, k, l, c, d, p, nl, np;
        
        n=in.nextInt();
        k=in.nextInt();
        l=in.nextInt();
        c=in.nextInt();
        d=in.nextInt();
        p=in.nextInt();
        nl=in.nextInt();
        np=in.nextInt();
        
        int ans1=k*l/nl;
        int ans2=c*d;
        int ans3=p/np;
        
        System.out.println( Math.min( ans1,Math.min(ans2,ans3) )/n );
        
    }
    
}
