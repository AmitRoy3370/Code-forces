import java.util.*;
public class GFG {
	public static void main (String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-->0){
		    int n = sc.nextInt();
		    char ch[] = sc.next().toCharArray();
		    Arrays.sort(ch);
		    System.out.println(String.valueOf(ch));
		}
		
	}
}
