import java.util.Scanner;

public class Main{
    
 static int testCases,n;
    
    static void solve(){
        
        for(int i=0;i<n;i++){
            
            System.out.print(1+" ");
            
        }
        
        System.out.println();
        
    }
    
    static Scanner in=new Scanner(System.in);
    
    public static void main(String[] amit){
        
        testCases=in.nextInt();
        
        for(int t=0;t<testCases;t++){
            
            n=in.nextInt();
            
            solve();
            
        }
        
    }
    
}
