import java.util.Scanner;
 
public class Main{
    
    public static void main(String[] amit ){
        
        Scanner in=new Scanner(System.in);
        
        long a=in.nextLong();
        long b=in.nextLong();
        
        System.out.println(a+" + "+b+" = "+(a+b) );
        System.out.println(a+" * "+b+" = "+(a*b) );
        System.out.println(a+" - "+b+" = "+(a-b) );
        
    }
    
}
