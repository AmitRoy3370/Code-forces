import java.util.Scanner;

public class Main{
    
    static Scanner in=new Scanner(System.in);
    
    public static void main(String [] amit){
        
        int n=in.nextInt();
        
        if(n%2==0){
            
            System.out.println("Mahmoud");
            
        }else{
            
            System.out.println("Bashar");
            
        }
        
        
    }
    
}
