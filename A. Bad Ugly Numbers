//package Points1000;

import java.util.*;

public class BadUglyNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		
		int t = sc.nextInt();
		
		while(t-->0)
		{
			int n = sc.nextInt();
			
			if(n==1)
				System.out.println("-1");
			else
			{
				System.out.print("2");
				for(int i=1;i<n;i++)
					System.out.print("3");
				System.out.println();
			}
		}
	}

}
