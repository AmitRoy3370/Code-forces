import java.util.Scanner;
//import java.util.Math;

public class Main{
    
    static Scanner in=new Scanner(System.in);
    
    public static void main(String[] amit){
        
        int a=in.nextInt();
        int b=in.nextInt();
        
 //       System.out.println( Math.abs( 3*a-2*b ) );
   
   int count=0;
   
   while(a<=b){
       
       count++;
       
       a*=3;
       b*=2;
       
   }
        
        System.out.println(count);
        
    }
    
}
