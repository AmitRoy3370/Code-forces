import java.util.*;
public class tesst {
	public static void main(String[] args){
		Scanner sc=new Scanner(System.in);
		int n = sc.nextInt()*sc.nextInt(),d=0;
		for (int i=0;i<n;i++) {
			if (sc.next().matches("[CMY]")) {
				d=1;
			}
		}
		System.out.println(d==0?"#Black&White":"#Color");
		
	
	}}
