import java.util.Scanner;

public class MyClass {
    
    static Scanner in=new Scanner(System.in);
    
    public static void main(String args[]) {
    
    int k=in.nextInt();
    int m=in.nextInt();
    int n=in.nextInt();
    
    double ans=(10.00*k-n)/m;
    
    System.out.println( (int)Math.ceil(ans) );
    
    }
}
